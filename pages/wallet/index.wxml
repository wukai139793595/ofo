<!--pages/wallet/index.wxml-->
<view class='container'>
  <view class='overage'>
    <view class=''>
      <text class='overage-header'>我的余额</text>
    </view>
    <view class=''>
      <text class='overage-content'>{{money}}</text>
    </view>
    <view class=''>
      <text class='overage-desc'>余额说明</text>
    </view>
  </view>
  <button class='btn-charge' bindtap='moveToCharge'>充值</button>
  <view class='my-ticket tapbar'>
    <text >我的用车券</text>
    <text>{{ticket}}张</text>
  </view>
    <view class='my-deposit tapbar'>
    <text >我的押金</text>
    <text class='color'>99元押金退款</text>
  </view>
    <view class='about tapbar'>
    <text >关于ofo</text>
    <text>></text>
  </view>
</view>