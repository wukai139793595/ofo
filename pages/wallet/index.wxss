/* pages/wallet/index.wxss */
.container{
  width: 100%;
  height: 100vh;
  background-color: #f2f2f2;
}
.overage{
  padding: 40rpx;
  background-color: white;
  text-align: center;
}
.overage .overage-header{
  font-size: 28rpx;
  padding: 20rpx 0;
}
.overage .overage-content{
  display: block;
  font-size: 100rpx;
  font-weight: 700;
  padding: 30rpx 0;
  margin: 30rpx 0;
}
.overage .overage-desc{
  border: 2px solid #666;
  border-radius: 60rpx;
  padding: 20rpx;
  font-size: 28rpx;
}
.btn-charge{
  width: 90%;
  margin-top: 40rpx;
  background-color: #b9dd08;
}
.tapbar{
    padding: 40rpx;
    background-color: #f2f2f2;
    margin-top: 40rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
}
.deposit text{
  margin-top: 2rpx;
}
.color{
  color: yellowgreen;
}